{
  "name": "cardsightai-demo-discord",
  "version": "1.0.0",
  "description": "A Discord bot demo showcasing CardSight AI's card identification capabilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint"
  },
  "keywords": [
    "discord",
    "bot",
    "cardsight",
    "trading-cards",
    "identification",
    "ai"
  ],
  "author": "CardSight AI",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "cardsightai": "^2.1.0",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
